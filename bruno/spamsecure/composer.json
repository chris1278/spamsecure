	"require": {
		"php": ">=7.0.0,<8.1.0",
		"phpbb/phpbb": ">=3.2.10,<3.4.0@dev",
		"composer/installers": "~1.0.0"
	},
	"extra": {
		"display-name": "Spamsecure by 69bruno",
